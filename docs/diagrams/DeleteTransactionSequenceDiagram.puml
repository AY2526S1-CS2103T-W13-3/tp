@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":DeleteTransactionCommandParser" as DeleteTransactionCommandParser LOGIC_COLOR
participant "c:DeleteTransactionCommand" as DeleteTransactionCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("deletetxn i/1 t/1")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("deletetxn i/1 t/1")
activate AddressBookParser

AddressBookParser -> DeleteTransactionCommandParser : parse("i/1 t/1")
activate DeleteTransactionCommandParser

create DeleteTransactionCommand
DeleteTransactionCommandParser -> DeleteTransactionCommand
activate DeleteTransactionCommand

DeleteTransactionCommand --> DeleteTransactionCommandParser
deactivate DeleteTransactionCommand

DeleteTransactionCommandParser --> AddressBookParser : c
deactivate DeleteTransactionCommandParser

AddressBookParser --> LogicManager : c
deactivate AddressBookParser

LogicManager -> DeleteTransactionCommand : execute(model)
activate DeleteTransactionCommand

DeleteTransactionCommand -> Model : setPerson(personToEdit, editedPerson)
activate Model

Model --> DeleteTransactionCommand
deactivate Model

create CommandResult
DeleteTransactionCommand -> CommandResult
activate CommandResult

CommandResult --> DeleteTransactionCommand
deactivate CommandResult

DeleteTransactionCommand --> LogicManager : result
deactivate DeleteTransactionCommand

[<--LogicManager
deactivate LogicManager
@enduml
