@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":SummaryCommandParser" as SummaryCommandParser LOGIC_COLOR
participant "c:SummaryCommand" as SummaryCommand LOGIC_COLOR
participant "result:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("summary")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("summary")
activate AddressBookParser

AddressBookParser -> SummaryCommandParser : parse("")
activate SummaryCommandParser

create SummaryCommand
SummaryCommandParser -> SummaryCommand
activate SummaryCommand

SummaryCommand --> SummaryCommandParser
deactivate SummaryCommand

SummaryCommandParser --> AddressBookParser : c
deactivate SummaryCommandParser

AddressBookParser --> LogicManager : c
deactivate AddressBookParser

LogicManager -> SummaryCommand : execute(model)
activate SummaryCommand

SummaryCommand -> Model : getFilteredPersonList()
activate Model

Model --> SummaryCommand : personList
deactivate Model

create CommandResult
SummaryCommand -> CommandResult
activate CommandResult

CommandResult --> SummaryCommand
deactivate CommandResult

SummaryCommand --> LogicManager : result
deactivate SummaryCommand

[<--LogicManager
deactivate LogicManager
@enduml
